-
--- Dumps geometries from collections and converts them to MARC21/XML
-SELECT
-  'dump geom -> convert to marc21',
-  gid,(ST_Dump(geom)).path[1],
-  ST_AsMARC21(
-    (ST_Dump(geom)).geom,'hdddmmss') AS marc21
-FROM asmarc21_rt
-ORDER BY gid;
